#include <iostream>

int main() {
   int num;
   int i=0;
   char y ='O';
   char n = 'X';
   int wrongnum = 0;
   int arr1[79]={2,4,2,2,4,3,4,1,1,2,3,1,1,4,3,2,4,1,2,4,5,5,3,4,1,3,3,5,5,2,2,5,2,5,1,3,3,4,2,2,3,5,1,4,3,1,1,4,2,3,4,4,5,5,3,1,5,4,4,5,4,1,3,2,4,3,5,2,4,2,2,4,1,2,5,2,5,2,1};
   int arr2[79]={2,4,4,2,4,3,4,2,1,4,2,1,1,2,3,2,4,3,2,2,5,3,3,4,1,3,1,5,5,2,4,5,2,5,1,3,3,4,3,2,2,2,1,5,3,1,1,2,2,3,4,2,5,5,2,4,5,4,4,2,4,3,2,5,4,5,5,4,4,5,2,5,3,2,5,1,4,2,5};
        
 
  for(i=0;i<79;i++){
      if(arr1[i]==arr2[i])
      printf("[%d]  %d %d   %c\n",i+1,arr1[i],arr2[i],y);
      else
      {printf("[%d]  %d %d   %c\n",i+1,arr1[i],arr2[i],n);
      wrongnum++;}
  }
    printf("wrongnum : %d",wrongnum);
    return 0;
}
